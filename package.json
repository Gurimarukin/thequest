{
  "name": "thequest",
  "version": "1.0.0",
  "description": "La quêêête",
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "targets": {
    "server": {
      "source": "src/server/index.ts",
      "engines": {
        "node": ">= 20"
      }
    },
    "client": {
      "source": "src/client/index.html",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "scripts": {
    "_node": "node --loader=./node_modules/commonjs-extension-resolution-loader/loader.js",
    "_swc": "swc src --out-dir tsbuild",
    "_nodemon": "nodemon -e ts,tsx,js,cjs,mjs,json,env --ignore dist/ --ignore tsbuild/ --ignore src/client/ --ignore uibook/",
    "clean": "rm -fr .parcel-cache dist",
    "prebuild": "yarn clean && yarn checkEnv",
    "build": "tsc -p tsconfig.build.json && parcel build",
    "checkEnv": "yarn _swc && yarn _node tsbuild/src/server/checkEnv.js && yarn _node tsbuild/src/client/checkEnv.js",
    "createUser": "yarn _swc && yarn _node tsbuild/src/server/createUser.js",
    "swcServer": "yarn _swc && yarn _node tsbuild/src/server/index.js",
    "~swcServer": "yarn _nodemon -x \"yarn swcServer\"",
    "~server": "yarn _nodemon -x \"yarn tsc -p tsconfig.server.json && yarn swcServer\"",
    "~client": "concurrently -k -n parcel,tsc -c green.bold,cyan.bold \"parcel src/client/index.html --dist-dir dist/client\" \"tsc -w --preserveWatchOutput -p tsconfig.client.json\"",
    "compile": "tsc -p tsconfig.json",
    "~compile": "yarn compile -w --preserveWatchOutput",
    "~uibook": "parcel uibook/uibook.html --dist-dir dist/uibook -p 5678",
    "_prettier": "prettier --cache --cache-strategy metadata .github src test uibook .*.cjs *.cjs *.mjs *.json .*.yml *.yml *.md",
    "formatCheck": "yarn _prettier --check",
    "format": "yarn _prettier --write",
    "lint": "eslint --cache --max-warnings 0 src test uibook",
    "lintFix": "yarn lint --fix",
    "test": "jest",
    "testUnit": "yarn test --testPathPattern=\".*\\.spec\\.ts\"",
    "testCoverage": "yarn test --ci --coverage",
    "dc": "depcruise src test",
    "dcGraph": "depcruise -v -T dot src -x '(node_modules|fs|util|fp\\.ts)' | dot -T svg | depcruise-wrap-stream-in-html > dependencygraph.html",
    "ci": "yarn tsc && yarn formatCheck && yarn lint && yarn dc && yarn testUnit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gurimarukin/thequest.git"
  },
  "author": "Gurimarukin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gurimarukin/thequest/issues"
  },
  "homepage": "https://github.com/Gurimarukin/thequest#readme",
  "dependencies": {
    "@phc/argon2": "1.0.9",
    "@popperjs/core": "2.11.8",
    "@react-spring/web": "9.7.5",
    "@use-gesture/react": "10.3.1",
    "cookie": "1.0.2",
    "dayjs": "1.11.13",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "fp-ts": "2.16.9",
    "fp-ts-contrib": "0.1.29",
    "fp-ts-routing": "0.6.0",
    "fp-ts-rxjs": "0.6.15",
    "got": "11.8.6",
    "history": "5.3.0",
    "hyper-ts": "0.7.9",
    "io-ts": "2.2.22",
    "jsdom": "26.0.0",
    "jsonwebtoken": "9.0.2",
    "ky": "0.33.3",
    "lodash.debounce": "4.0.8",
    "lua-in-js": "2.2.0",
    "mongodb": "6.13.0",
    "monocle-ts": "2.3.13",
    "newtype-ts": "0.3.5",
    "qs": "6.14.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-popper": "2.3.0",
    "react-select": "5.10.0",
    "rxjs": "6.6.7",
    "rxjs-stream": "3.3.0",
    "swr": "2.3.2",
    "type-fest": "4.35.0",
    "upash": "1.0.2",
    "uuid": "11.0.5",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.6",
    "@eslint/js": "9.20.0",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.10.18",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@type-config/strict": "1.2.1",
    "@types/cookie": "1.0.0",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.14",
    "@types/jsdom": "21.1.7",
    "@types/jsonwebtoken": "9.0.8",
    "@types/lodash.debounce": "4.0.9",
    "@types/node": "18.19.76",
    "@types/qs": "6.9.18",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/uuid": "10.0.0",
    "@types/xml2js": "0.4.14",
    "autoprefixer": "10.4.20",
    "commonjs-extension-resolution-loader": "0.1.0",
    "concurrently": "9.1.2",
    "dependency-cruiser": "16.10.0",
    "eslint": "9.20.1",
    "eslint-plugin-fp-ts": "0.3.2",
    "eslint-plugin-functional": "8.0.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "eslint-plugin-tailwindcss": "3.18.0",
    "jest": "29.7.0",
    "nodemon": "3.1.9",
    "parcel": "2.13.3",
    "postcss": "8.5.3",
    "prettier": "3.5.1",
    "prettier-plugin-tailwindcss": "0.6.11",
    "process": "0.11.10",
    "punycode": "2.3.1",
    "querystring-es3": "0.2.1",
    "sodium": "3.0.2",
    "tailwindcss": "3.4.17",
    "ts-jest": "29.2.5",
    "tslib": "2.8.1",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.1",
    "url": "0.11.4"
  }
}
